[user]
  name = Rob Romito
  email = rob@robertromito.com

[core]
  autocrlf = input

[push]
  default = simple

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p

[credential]
	helper = osxkeychain
